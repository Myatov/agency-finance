{
  "name": "agency-finance",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:check": "npm run build && test -d .next && test -f .next/BUILD_ID",
    "postinstall": "prisma generate",
    "prestart": "tsx scripts/ensure-prisma-client.ts || true",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push-periods": "prisma db execute --file prisma/add-work-periods-invoices-payments.sql",
    "db:push-income-period": "prisma db execute --file prisma/add-income-work-period-id.sql",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate-server": "tsx scripts/run-migrate-cost-categories.ts",
    "clean": "rm -rf .next node_modules/.cache",
    "fresh": "npm run clean && npm run build"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "@prisma/client": "^5.19.0",
    "bcryptjs": "^2.4.3",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/bcryptjs": "^2.4.6",
    "typescript": "^5.5.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
    "prisma": "^5.19.0",
    "tsx": "^4.16.0"
  }
}
